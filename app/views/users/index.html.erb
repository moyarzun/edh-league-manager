<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show ", user %>
      <%= link_to('| Delete', user, data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' } ) unless user.is_admin? %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>
